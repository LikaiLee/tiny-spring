<?ioc.xml version="1.0" encoding="utf-8" ?>
<beans>
    <bean name="helloWorldService" class="service.impl.HelloWorldServiceImpl">
        <property name="text" value="hello world!"/>
        <property name="outputService" ref="outputService"/>
    </bean>

    <bean name="outputService" class="service.OutputService">
        <property name="helloWorldService" ref="helloWorldService"/>
    </bean>

<!--    <bean name="customProcessor" class="context.CustomBeanPostProcessor"/>-->
    <bean name="beanInitializeLogger" class="context.BeanInitializeLogger"/>
</beans>